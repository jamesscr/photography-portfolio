{
  "name": "photography-portfolio",
  "version": "1.0.0",
  "description": "Photography Portfolio",
  "main": "server.js",
  "scripts": {
      "clean": "rm -rf build",
      "prebuild": "npm run clean",
      "build:css": "tailwindcss -i ./src/input.css -o ./build/output.css",
      "build:html": "cp index.html about_me.html contact.html build/",
      "build:assets": "cp -R dist/assets build/",
      "build": "npm run prebuild && npm run build:css && npm run build:html && npm run build:assets",
      "start": "node server.js",
      "dev": "nodemon server.js",
      "prod": "npm run build && npm start"
    },
  "keywords": [],
  "author": "James Allan",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^3.0.4",
    "tailwindcss": "^3.3.3",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "express": "^4.18.2",
    "nodemailer": "^6.9.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1"
  }
}